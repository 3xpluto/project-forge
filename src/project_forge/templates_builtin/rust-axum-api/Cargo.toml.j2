[package]
name = "{{ project_name }}"
version = "0.1.0"
edition = "2021"
authors = ["{{ author }}"]
description = "{{ description }}"

[dependencies]
axum = "0.7"
tokio = { version = "1", features = ["macros", "rt-multi-thread"] }
serde = { version = "1", features = ["derive"] }
serde_json = "1"
tracing = "0.1"
tracing-subscriber = { version = "0.3", features = ["env-filter"] }

[dev-dependencies]
tower = "0.5"
http = "1"
